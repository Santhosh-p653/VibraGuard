import pandas as pd
import matplotlib.pyplot as plt
import config 

df = pd.read_csv(config.y)
plt.figure(figsize=(8, 5))
plt.scatter(df['svm_mean'], df['step_count'], color='purple', alpha=0.6)
plt.title('Motion Intensity vs Step Count')
plt.xlabel('SVM Mean')
plt.ylabel('Step Count')
plt.grid(True)
plt.tight_layout()
plt.show()